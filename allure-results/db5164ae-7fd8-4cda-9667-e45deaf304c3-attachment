	<form action="" style="margin-bottom:3rem;"> 
	  <div class="input-group">
		<input type="text" class="form-control form-control-sm" style="font-size:11px; text-transform: uppercase;" name="coupon" value="" placeholder="Enter your coupon" id="input-coupon">
		<div class="input-group-append">
		  
		  
		   
		   <a class="btn btn-sm btn-primary" href="javascript:;" onclick="javascript: applyCouponCode();" data-loading-text="Loading...">Apply Coupon</a> 		  
