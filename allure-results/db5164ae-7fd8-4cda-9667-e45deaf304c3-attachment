<script type="text/javascript">
function reloadPage()
{
  setTimeout(function () { window.location.reload(); }, 1000);

}

function showQty()
{
    $('.clsLowQty').css('border', '1px solid red');
    $('.clsLowQty td').css('border-top', '1px solid red');
    
    $('.clsLowQty').attr('id', 'clsErrorBorder');
    
    $('html, body').animate({
        scrollTop: $('.clsLowQty').offset().top - 60 //#DIV_ID is an example. Use the id of your destination on the page
    }, 'slow');
    
}

function clearCart()
{
   $.ajax({
		url: 'index.php?route=checkout/cart/clearcart/en',
		type: 'post',		
		beforeSend: function() {
			$('#clsRecentList').html('<img src="catalog/view/theme/default/image/processing.gif" />');
		},
		success: function(data) {
			//$('#clsRecentList').html(data);
			window.location.reload();
		}
	});
}

function clearCookie()
{
   $.ajax({
		url: 'index.php?route=product/product/clearCookie/en',
		type: 'post',		
		beforeSend: function() {
			$('#clsRecentList').html('<img src="catalog/view/theme/default/image/processing.gif" />');
		},
		success: function(data) {
			$('#clsRecentList').html(data);
		}
	});
}


</script>

